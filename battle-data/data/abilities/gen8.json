  "mirrorarmor": { "name": "Mirror Armor", "flags": [] },
  "gulpmissile": { "name": "Gulp Missile", "flags": ["Permanent"] },
  "stalwart": { "name": "Stalwart", "flags": [] },
  "steamengine": { "name": "Steam Engine", "flags": [] },
  "punkrock": { "name": "Punk Rock", "flags": [] },
