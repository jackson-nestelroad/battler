{
  "masterball": {
    "name": "Master Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": { "on_modify_catch_rate": ["return $catch_rate * 255"] }
    }
  },
  "ultraball": {
    "name": "Ultra Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": { "on_modify_catch_rate": ["return $catch_rate * 2"] }
    }
  },
  "greatball": {
    "name": "Great Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": { "on_modify_catch_rate": ["return $catch_rate * 3/2"] }
    }
  },
  "pokeball": {
    "name": "Pok√© Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"]
  },
  "safariball": { "name": "Safari Ball", "flags": ["Ball"] },
  "netball": {
    "name": "Net Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $target.types hasany [water, bug]:",
          ["return $catch_rate * 7/2"]
        ]
      }
    }
  },
  "diveball": {
    "name": "Dive Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $field.environment == water:",
          ["return $catch_rate * 7/2"]
        ]
      }
    }
  },
  "nestball": {
    "name": "Nest Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "$level = $target.level",
          "if $level < 1 or $level > 29:",
          ["return"],
          "return $catch_rate * ((41 - $level) / 10)"
        ]
      }
    }
  },
  "repeatball": {
    "name": "Repeat Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "$base_species = func_call(base_species: $target)",
          "if func_call(has_species_registered: $source.player $base_species):",
          ["return $catch_rate * 7/2"]
        ]
      }
    }
  },
  "timerball": {
    "name": "Timer Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "return $catch_rate * func_call(min: expr(1 + $battle.turn * 3/10) 4)"
        ]
      }
    }
  },
  "luxuryball": {
    "name": "Luxury Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "condition": {
      "callbacks": {
        "on_modify_friendship_increase": ["return $friendship + 1"]
      }
    }
  },
  "premierball": { "name": "Premier Ball", "flags": ["Ball"] },
  "duskball": {
    "name": "Dusk Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $field.environment == cave or $field.time == evening or $field.time == night:",
          ["return $catch_rate * 3"]
        ]
      }
    }
  },
  "healball": {
    "name": "Heal Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_catch": [
          "# NOTE: This is not logged. The Mon has exited the battle so this is just for the caught data the player receives.",
          "force_fully_heal: $target"
        ]
      }
    }
  },
  "quickball": {
    "name": "Quick Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $battle.turn == 1:",
          ["return $catch_rate * 5"]
        ]
      }
    }
  },
  "cherishball": {
    "name": "Cherish Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"]
  },
  "lureball": {
    "name": "Lure Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $target.player.wild_encounter_type == fishing:",
          ["return $catch_rate * 4"]
        ]
      }
    }
  },
  "levelball": {
    "name": "Level Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $source.level <= $target.level:",
          ["return"],
          "if $source.level < $target.level * 2:",
          ["return $catch_rate * 2"],
          "if $source.level < $target.level * 4:",
          ["return $catch_rate * 4"],
          "return $catch_rate * 8"
        ]
      }
    }
  },
  "moonball": {
    "name": "Moon Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "$evolve_by_moon_stone = [nidorina, nidorino, clefairy, jigglypuff, skitty, munna]",
          "$base_species = func_call(base_species: $target)",
          "if $evolve_by_moon_stone has $base_species:",
          ["return $catch_rate * 4"]
        ]
      }
    }
  },
  "heavyball": {
    "name": "Heavy Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_species_catch_rate": [
          "if $target.weight < 100:",
          ["if $catch_rate < 20:", ["return 0"], "return $catch_rate - 20"],
          "if $target.weight < 200:",
          ["return $catch_rate"],
          "if $target.weight < 300:",
          ["return $catch_rate + 20"],
          "return $catch_rate + 30"
        ]
      }
    }
  },
  "fastball": {
    "name": "Fast Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $target.base_stats.spe >= 100:",
          ["return $catch_rate * 4"]
        ]
      }
    }
  },
  "friendball": {
    "name": "Friend Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_catch": ["set_friendship: $mon 150"]
      }
    }
  },
  "loveball": {
    "name": "Love Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $target.gender == u or $source.gender == u:",
          ["return"],
          "if $target.gender != $source.gender:",
          ["return $catch_rate * 8"]
        ]
      }
    }
  },
  "parkball": {
    "name": "Park Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"]
  },
  "sportball": {
    "name": "Sport Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"]
  },
  "dreamball": {
    "name": "Dream Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "if $target.is_asleep:",
          ["return $catch_rate * 4"]
        ]
      }
    }
  },
  "beastball": {
    "name": "Beast Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"],
    "effect": {
      "callbacks": {
        "on_modify_catch_rate": [
          "$base_species = func_call(base_species: $target)",
          "if func_call(species_has_flag: $base_species ultrabeast):",
          ["return $catch_rate * 5"],
          "return $catch_rate * 1/10"
        ]
      }
    }
  },
  "strangeball": {
    "name": "Strange Ball",
    "target": "IsolatedFoe",
    "flags": ["Ball"]
  }
}
