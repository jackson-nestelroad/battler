  "mentalherb": {
    "name": "Mental Herb",
    "fling": { "power": 10 },
    "flags": [],
    "effect": {
      "callbacks": {
        "on_update": [
          "$activate = false",
          "$conditions = [attract, taunt, encore, torment, disable, healblock]",
          "foreach $condition in $conditions:",
          [
            "if func_call(has_volatile: $mon $condition):",
            ["$activate = true"]
          ],
          "if $activate:",
          ["use_item: $mon"]
        ],
        "on_use": [
          "$conditions = [attract, taunt, encore, torment, disable, healblock]",
          "foreach $condition in $conditions:",
          ["remove_volatile: $mon $condition"]
        ]
      }
    }
  },
