  "mon": {
    "name": "Mon",
    "condition_type": "Built-in",
    "condition": {
      "callbacks": {
        "on_type_immunity": [
          "if $type == ground and !$mon.is_grounded:",
          ["return false"]
        ],
        "on_negate_immunity": [
          "if $type == ground and $mon.is_grounded:",
          ["return false"]
        ],
        "suppress_mon_ability": {
          "priority": 10,
          "program": [
            "if $mon.transformed and func_call(ability_has_flag: $mon.ability notransform):",
            ["return true"],
            "if func_call(ability_has_flag: $mon.ability permanent):",
            ["return false"]
          ]
        },
        "on_modify_catch_rate": {
          "priority": 1,
          "program": [
            "$base_species = func_call(base_species: $target)",
            "if func_call(species_has_flag: $base_species ultrabeast):",
            [
              "if $effect.id == beastball or $effect.id == masterball:",
              ["return"],
              "skip_effect_callback: $effect.id",
              "return $catch_rate * 1/10"
            ]
          ]
        }
      }
    }
  },
