  "powertrick": {
    "name": "Power Trick",
    "category": "Status",
    "primary_type": "Psychic",
    "pp": 10,
    "accuracy": "exempt",
    "target": "User",
    "flags": ["Snatch"],
    "hit_effect": {
      "volatile_status": "powertrick"
    },
    "condition": {
      "callbacks": {
        "on_start": [
          "if !$effect_state.logged:",
          ["log_start", "$effect_state.logged = true"],
          "$atk = $target.stats.def",
          "$def = $target.stats.atk",
          "$target.stats.atk = $atk",
          "$target.stats.def = $def"
        ],
        "on_copy_volatile": [
          "$effect_state.logged = true",
          "run_event_on_mon_volatile: $this.id Start"
        ],
        "on_end": [
          "log_end",
          "$effect_state.logged = true",
          "run_event_on_mon_volatile: $this.id Start"
        ],
        "on_restart": ["remove_volatile: $target $this.id", "return true"]
      }
    }
  },
