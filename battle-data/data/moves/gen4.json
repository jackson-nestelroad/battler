  "acupressure": {
    "name": "Acupressure",
    "category": "Status",
    "primary_type": "Normal",
    "pp": 30,
    "accuracy": "exempt",
    "target": "AdjacentAllyOrUser",
    "flags": [],
    "effect": {
      "callbacks": {
        "on_hit": [
          "$potential = []",
          "foreach $stat in func_call(boostable_stats):",
          [
            "if func_call(get_boost: $target.boosts $stat) < 6:",
            ["$potential = func_call(append: $potential $stat)"]
          ],
          "if $potential.is_empty:",
          ["return false"],
          "$stat = func_call(sample: $potential)",
          "$boosts = func_call(boost_table)",
          "$boosts = func_call(set_boost: $boosts $stat 2)",
          "boost: $target $boosts"
        ]
      }
    }
  },
