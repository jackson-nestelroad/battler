{
  "firegem": {
    "name": "Fire Gem",
    "flags": ["Gem"],
    "effect": {
      "callbacks": {
        "on_source_try_primary_hit": {
          "order": 1,
          "program": [
            "if $target == $source or $move.category == status:",
            ["return"],
            "if $move.type == fire:",
            ["use_item: $source"]
          ]
        },
        "on_use": ["add_volatile: $mon gem"]
      }
    }
  },
